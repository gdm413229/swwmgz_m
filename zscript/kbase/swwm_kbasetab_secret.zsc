Class DemolitionistSecretTab : DemolitionistMenuTab
{
	override DemolitionistMenuTab Init( DemolitionistMenu master )
	{
		title = "$SWWM_SECRETTAB";
		bHidden = true;
		return Super.Init(master);
	}

	override void OnSelect()
	{
		bHidden = false;
	}

	override void OnDeselect()
	{
		bHidden = true;
	}
}
